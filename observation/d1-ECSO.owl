    <!-- http://www.purl.dataone.org/odo#ECSO_0000010051 -->

    <owl:Class rdf:about="http://www.purl.dataone.org/odo#ECSO_0000010051">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01000155"/>
        <odo:ECSO_01000001>Elizabeth Olson, http://orcid.org/0000-0001-8622-9698</odo:ECSO_01000001>
        <odo:ECSO_01000003>http://www.ozcoasts.gov.au/indicators/sediment_org_matter.jsp</odo:ECSO_01000003>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Elizabeth Olson</dc:creator>
        <rdfs:label xml:lang="en">refactory organic material</rdfs:label>
        <skos:definition>organic compounds (woody debris made of lignin and cellulose) have very high TOC:TN ratios and are highly resistant to degradation</skos:definition>
    </owl:Class>
    


    <!-- http://www.purl.dataone.org/odo#ECSO_0000010052 -->

    <owl:Class rdf:about="http://www.purl.dataone.org/odo#ECSO_0000010052">
