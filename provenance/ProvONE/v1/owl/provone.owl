    <!-- http://purl.org/provone/v1/ontology#controls -->

    <owl:ObjectProperty rdf:about="http://purl.org/provone/v1/ontology#controls">
        <rdfs:label>controls</rdfs:label>
        <prov:category>Relates a Controller to its destination Program.</prov:category>
        <prov:category>workflow-specification</prov:category>
        <rdfs:range rdf:resource="http://purl.org/provone/v1/ontology#Program"/>
        <rdfs:domain rdf:resource="http://purl.org/provone/v1/ontology#Controller"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/provone/v1/ontology#hasLocation -->

    <owl:ObjectProperty rdf:about="http://purl.org/provone/v1/ontology#hasLocation">
        <rdfs:label>hasLocation</rdfs:label>
        <rdfs:comment>Connects a Location to a Port of a Program. The port may be semantically typed as an input port or an output port, depending on its defined relationship with the Program.</rdfs:comment>
        <prov:category>workflow-specification</prov:category>
        <rdfs:domain rdf:resource="http://purl.org/provone/v1/ontology#Port"/>
        <rdfs:range rdf:resource="&prov;Location"/>
    </owl:ObjectProperty>
        


    <!-- http://purl.org/provone/v1/ontology#hasDefaultParam -->

    <owl:ObjectProperty rdf:about="http://purl.org/provone/v1/ontology#hasDefaultParam">
        <rdfs:label>hasDefaultParam</rdfs:label>
        <prov:category>workflow-specification</prov:category>
        <rdfs:comment>Specifies that a given Port (likely a semantically typed input port via hasInPort) has a certain Entity item (usually Data) as a default parameter. This enables Workflow instances to be configured.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/provone/v1/ontology#Port"/>
        <rdfs:range rdf:resource="&prov;Entity"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/provone/v1/ontology#hasInPort -->

    <owl:ObjectProperty rdf:about="http://purl.org/provone/v1/ontology#hasInPort">
        <rdfs:label>hasInPort</rdfs:label>
        <rdfs:comment>Enables the specification of a Port as an input port of a particular Program.</rdfs:comment>
        <prov:category>workflow-specification</prov:category>
        <rdfs:range rdf:resource="http://purl.org/provone/v1/ontology#Port"/>
        <rdfs:domain rdf:resource="http://purl.org/provone/v1/ontology#Program"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/provone/v1/ontology#hasOutPort -->

    <owl:ObjectProperty rdf:about="http://purl.org/provone/v1/ontology#hasOutPort">
        <rdfs:label>hasOutPort</rdfs:label>
        <prov:category>workflow-specification</prov:category>
        <rdfs:comment>Enables the specification of a Port as an output port of a particular Program.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/provone/v1/ontology#Program"/>
        <rdfs:range rdf:resource="http://purl.org/provone/v1/ontology#Port"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/provone/v1/ontology#hasSubProgram -->

    <owl:ObjectProperty rdf:about="http://purl.org/provone/v1/ontology#hasSubProgram">
        <rdfs:label>hasSubProgram</rdfs:label>
        <prov:category>workflow-specification</prov:category>
        <rdfs:comment>Specifies the recursive composition of Programs, where a parent Program includes a child Program as part of its specification.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/provone/v1/ontology#Program"/>
        <rdfs:range rdf:resource="http://purl.org/provone/v1/ontology#Program"/>
    </owl:ObjectProperty>
        


    <!-- http://purl.org/provone/v1/ontology#wasPartOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/provone/v1/ontology#wasPartOf">
        <rdfs:label>wasPartOf</rdfs:label>
        <prov:category>trace-specification</prov:category>
        <rdfs:comment>Specifies the structure of Execution instances, where a parent Execution (associated with a Workflow) had child Executions (associated with Programs and subworkflows).</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.org/provone/v1/ontology#Execution"/>
        <rdfs:domain rdf:resource="http://purl.org/provone/v1/ontology#Execution"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/provone/v1/ontology#controlledBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/provone/v1/ontology#controlledBy">
        <rdfs:label>controlledBy</rdfs:label>
        <prov:category>workflow-specification</prov:category>
        <rdfs:comment>Relates a Program to the controller it is controlled by.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/provone/v1/ontology#Program"/>
        <rdfs:range rdf:resource="http://purl.org/provone/v1/ontology#Controller"/>
    </owl:ObjectProperty>
    


