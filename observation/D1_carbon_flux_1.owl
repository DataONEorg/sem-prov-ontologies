    <!-- http://www.semanticweb.org/ontologies/2014/10/D1_CarbonFlux#OWLClass_d4cc57a9_1c01_4403_b016_7c6132b1ed48 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2014/10/D1_CarbonFlux#OWLClass_d4cc57a9_1c01_4403_b016_7c6132b1ed48">
