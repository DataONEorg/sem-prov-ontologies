    <!-- http://www.semanticweb.org/ontologies/2014/10/D1_CarbonFlux#TEM6_MstmipVersion1ModeledMethod -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2014/10/D1_CarbonFlux#TEM6_MstmipVersion1ModeledMethod">
