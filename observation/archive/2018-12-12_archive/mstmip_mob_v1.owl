    <!-- http://www.semanticweb.org/ontologies/2014/10/primary_production_terms_mob#OWLClass_903806ae_a331_4bea_8b44_9ca38b64ef75 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2014/10/primary_production_terms_mob#OWLClass_903806ae_a331_4bea_8b44_9ca38b64ef75">
        <rdfs:label>GrassCarbonPool</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2014/10/primary_production_terms_mob#OWLClass_49ad0bd6_ecb1_47ea_8b21_150be66fcfe6"/>
