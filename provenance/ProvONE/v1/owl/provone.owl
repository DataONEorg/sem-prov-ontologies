    <!-- http://purl.org/provone/v1/ontology#Data -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#Data">
        <rdfs:label>Data</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <rdfs:comment>A Data item represents the basic unit of information consumed and produced by a Program. Multiple Data items may be grouped into a Collection.</rdfs:comment>
        <prov:category>data-specification</prov:category>
    </owl:Class>
    


    <!-- http://purl.org/provone/v1/ontology#Visualization -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#Visualization">
        <rdfs:label>Visualization</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <rdfs:comment>A Visualization represents a visual product produced by a Program.</rdfs:comment>
        <prov:category>data-specification</prov:category>
    </owl:Class>
    


    <!-- http://purl.org/provone/v1/ontology#Document -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#Document">
        <rdfs:label>Document</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <rdfs:comment>A Document represents a scientific work that was created as a result of one or more Executions.  This may be a published journal article, a government report, etc.</rdfs:comment>
        <prov:category>data-specification</prov:category>
    </owl:Class>
    


    <!-- http://purl.org/provone/v1/ontology#DataLink -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#DataLink">
        <rdfs:label>DataLink</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <rdfs:comment>A DataLink enables to send Data items between Programs, always connecting an OutputPort from one Program to the InputPort in another Program. It is possibe for an OutputPort as well as for an InputPort to be the source or destination of multiple DataLinks, respectively.</rdfs:comment>
        <prov:category>workflow-specification</prov:category>
    </owl:Class>
    


    <!-- http://purl.org/provone/v1/ontology#InputPort -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#InputPort">
        <rdfs:label>InputPort</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <prov:category>An InputPort enables a Program to receive input Data items, which in turn may be produced by other Programs and sent through DataLinks or specified as default parameters.</prov:category>
        <prov:category>workflow-structure</prov:category>
    </owl:Class>
    


    <!-- http://purl.org/provone/v1/ontology#OutputPort -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#OutputPort">
        <rdfs:label>OutputPort</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <prov:category>workflow-specification</prov:category>
        <prov:category>An OutputPort enables a Program to produce output Data items, which may be forwarded to other Programs via DataLinks.</prov:category>
    </owl:Class>
    


    <!-- http://purl.org/provone/v1/ontology#Program -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#Program">
        <rdfs:label>Program</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <rdfs:subClassOf rdf:resource="&prov;Plan"/>
        <prov:definition>A Program represents a computational task that consumes and produces data through its input and output ports, respectively. It can be atomic or composite, the latter case represented by a possibly nested Program. It is also subject to versioning by prov:wasDerivedFrom.</prov:definition>
        <prov:category>workflow-specification</prov:category>
    </owl:Class>
    


    <!-- http://purl.org/provone/v1/ontology#Execution -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#Execution">
        <rdfs:label>Execution</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Activity"/>
        <rdfs:comment>An Execution represents the execution of a Program. If the Program in question is a Workflow, then the Execution represents a trace of its execution.</rdfs:comment>
        <prov:category>trace-specification</prov:category>
    </owl:Class>
    


    <!-- http://purl.org/provone/v1/ontology#Controller -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#Controller">
        <rdfs:label>Controller</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <prov:category>A Controller specifies a control link between two Programs, and defines the model of computation for the Program.  Some models will implement sequential flow control, others will implement parallel flow control, etc.  Each model of computation can be implemented by subclassing the Controller class.</prov:category>
        <prov:category>workflow-specification</prov:category>
    </owl:Class>
    


    <!-- http://purl.org/provone/v1/ontology#User -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#User">
        <rdfs:label>User</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Agent"/>
        <prov:category>user-specification</prov:category>
        <prov:definition>An User is the person responsible for the specification of a Program, which in turn can be a Workflow. Its specification serves attribution and accountability purposes.</prov:definition>
    </owl:Class>
    


    <!-- http://purl.org/provone/v1/ontology#Workflow -->

    <owl:Class rdf:about="http://purl.org/provone/v1/ontology#Workflow">
        <rdfs:label>Workflow</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.org/provone/v1/ontology#Program"/>
        <prov:category>workflow-specification</prov:category>
        <prov:definition>A Workflow is a distinguished Program, which indicates that is meant to represent the computational experiment in its entirety. It is also subject to versioning by prov:wasDerivedFrom through its super-class provone:Program.</prov:definition>
    </owl:Class>
</rdf:RDF>



