    <!-- http://www.semanticweb.org/ontologies/2014/10/primary_production_terms_mob#OWLClass_76b459c6_1fa9_4274_9ba8_c089405d82f9 -->

    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2014/10/primary_production_terms_mob#OWLClass_76b459c6_1fa9_4274_9ba8_c089405d82f9">
        <rdfs:label>LeafCarbonPool</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/ontologies/2014/10/primary_production_terms_mob#OWLClass_49ad0bd6_ecb1_47ea_8b21_150be66fcfe6"/>
