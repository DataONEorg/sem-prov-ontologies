    <!-- http://www.purl.dataone.org/odo#ECSO_0000010050 -->

    <owl:Class rdf:about="http://www.purl.dataone.org/odo#ECSO_0000010050">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ENVO_01000155"/>
        <odo:ECSO_01000001>Elizabeth Olson, https://orcid.org/0000-0001-8622-9698</odo:ECSO_01000001>
